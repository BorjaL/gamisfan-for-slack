{
  "name": "gamis-for-slack",
  "version": "0.1.0",
  "description": "Gamisfan for slack",
  "main": "index.js",
  "author": "@BorjaL",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config $NODE_DEBUG_OPTION dist",
    "build": "rm -rf dist && npm run lint && tsc",
    "serve": "yarn build && node -r dotenv/config $NODE_DEBUG_OPTION dist",
    "test": "jest --verbose",
    "lint": "tslint -c tslint.json -p tsconfig.json --force",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix --force",
    "tsc": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@slack/web-api": "^5.4.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger-winston": "^0.0.2",
    "@types/koa-router": "^7.0.42",
    "@types/qs": "^6.9.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.7.0",
    "http-status-codes": "^1.4.0",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa2-winston": "^3.1.1",
    "qs": "^6.9.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/dist/"
    ],
    "preset": "ts-jest",
    "testMatch": null,
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
